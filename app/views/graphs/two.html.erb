<%
header = @header
%>
<%= form_tag 'two' do%>
Select the heading for filter: <%= select_tag :filter, options_for_select(header), :prompt=> 'Select', :class => 'select2 form-control', :onchange => "this.form.submit();" %><br>
<%end%>
<%= form_tag 'three'  do %>
<%if @test%>
  
  Select data to filter: <%= select_tag :chosen, options_for_select(@values), :class => 'select2 form-control', :multiple => true %><br>
  <%end%>
 Select the heading for the x axis: <%= select_tag :x, options_for_select(header), :class => 'select2 form-control' %><br>
 Select the heading for the y axis: (must be numerical) <%= select_tag :y, options_for_select(header), :class => 'select2 form-control' %><br>
  
  <%= submit_tag "Import", :class=>"btn orange-background"%>
<% end %>

